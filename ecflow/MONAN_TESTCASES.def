suite MONAN_TESTCASES
   defstatus suspended

   edit ECF_HOME "/mnt/beegfs/monan/MONAN-WorkFlow/ecflow"
   edit ECF_HOST "egeon-login1.cptec.inpe.br"
   edit ECF_INCLUDE "/mnt/beegfs/monan/MONAN-WorkFlow/ecflow/includes"
   edit ECF_TRIES "1"

   edit EXP "GFS"
   edit RES "1024002"
   edit FCSTH "240"
   
   family GAM_TC
   
      family P1
         repeat date YYYYMMDDP 20240420 20240501 

         task pre
            label Info "Tarefa que executa o script 2.run_pre.bash de scripts_CD-CT."
            label date "YYYYMMDDHH"
   
          task model
             trigger /MONAN_TESTCASES/GAM_TC/P1/pre eq complete
             label Info "Tarefa que executa o script 3.run_model.bash de scripts_CD-CT."
             label date "YYYYMMDDHH"
             
          task post
             trigger  /MONAN_TESTCASES/GAM_TC/P1/model eq complete
             label Info "Tarefa que executa o script 4.run_post.bash de scripts_CD-CT."
             label date "YYYYMMDDHH"

          task files_management
             trigger /MONAN_TESTCASES/GAM_TC/P1/post eq complete
             label Info "Tarefa que gerencia os arquivos gerados em disco."

      endfamily


      family P2
         trigger /MONAN_TESTCASES/GAM_TC/P1 eq complete
         repeat date YYYYMMDDP 20231222 20240130 

         task pre
            label Info "Tarefa que executa o script 2.run_pre.bash de scripts_CD-CT."
            label date "YYYYMMDDHH"

          task model
             trigger /MONAN_TESTCASES/GAM_TC/P2/pre eq complete
             label Info "Tarefa que executa o script 3.run_model.bash de scripts_CD-CT."
             label date "YYYYMMDDHH"
             
          task post
             trigger  /MONAN_TESTCASES/GAM_TC/P2/model eq complete
             label Info "Tarefa que executa o script 4.run_post.bash de scripts_CD-CT."
             label date "YYYYMMDDHH"

          task files_management
             trigger /MONAN_TESTCASES/GAM_TC/P2/post eq complete
             label Info "Tarefa que gerencia os arquivos gerados em disco."
   
      endfamily

      family P3
         trigger /MONAN_TESTCASES/GAM_TC/P2 eq complete
         repeat date YYYYMMDDP 20240621 20240730 

         task pre
            label Info "Tarefa que executa o script 2.run_pre.bash de scripts_CD-CT."
            label date "YYYYMMDDHH"
   
          task model
             trigger /MONAN_TESTCASES/GAM_TC/P3/pre eq complete
             label Info "Tarefa que executa o script 3.run_model.bash de scripts_CD-CT."
             label date "YYYYMMDDHH"
             
          task post
             trigger  /MONAN_TESTCASES/GAM_TC/P3/model eq complete
             label Info "Tarefa que executa o script 4.run_post.bash de scripts_CD-CT."
             label date "YYYYMMDDHH"
          
          task files_management
             trigger /MONAN_TESTCASES/GAM_TC/P3/post eq complete
             label Info "Tarefa que gerencia os arquivos gerados em disco."

      endfamily
      
   endfamily




   
endsuite
